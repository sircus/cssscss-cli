{
  "version": "0.2.0",
  "name": "sircus-cssscss-cli",
  "description": "convert to scss from css4 syntax, using css-scss",
  "license": "MIT",
  "homepage": "http://sircus.blivesta.com",
  "author": {
    "name":"blivesta",
    "url": "http://blivesta.com"
  },
  "keywords": [
    "css4",
    "scss",
    "css-scss",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sircus-cssscss-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sircus-cssscss-cli/issues"
  },
  "dependencies": {
    "commander":"^2.8.1",
    "chalk":"^1.0.0",
    "css-scss": "^0.0.7"
  },
  "scripts": {
    "clean": "rm -rf ./sandbox/output.scss",
    "cssscss": "node .",
    "cssscss:cli":"sircus-cssscss sandbox/input.css sandbox/_output.scss",
    "build": "npm run clean && npm run cssscss:cli"
  },
  "bin": {
    "cssscss": "bin/cssscss.js"
  }
}
